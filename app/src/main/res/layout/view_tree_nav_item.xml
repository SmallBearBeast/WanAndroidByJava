<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/treeNavTitleTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_5C5C5C"
        android:textSize="@dimen/sp_15"
        android:textStyle="bold"
        tools:text="开发环境" />

    <com.bear.wanandroidbyjava.Widget.FlowLayout
        android:id="@+id/treeNavLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        app:fl_horizontal_space="@dimen/dp_12"
        app:fl_vertical_space="@dimen/dp_12" />
</LinearLayout>